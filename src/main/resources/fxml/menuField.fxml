<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="anchorPane" prefHeight="640.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Button fx:id="scrambleButton" layoutX="25.0" layoutY="21.0" mnemonicParsing="false" onAction="#scrambleCube" prefHeight="36.0" prefWidth="150.0" stylesheets="@../styles/Styles.css" text="Scramble">
         <effect>
            <DropShadow />
         </effect></Button>
      <Button fx:id="resetButton" layoutX="25.0" layoutY="81.0" mnemonicParsing="false" onAction="#resetCube" prefHeight="36.0" prefWidth="150.0" stylesheets="@../styles/Styles.css" text="Reset">
         <effect>
            <DropShadow />
         </effect></Button>
      <ComboBox fx:id="cubeSizeSetter" layoutX="103.0" layoutY="141.0" onAction="#setCubeSize" prefHeight="36.0" prefWidth="72.0" stylesheets="@../styles/Styles.css">
         <effect>
            <DropShadow />
         </effect></ComboBox>
      <Button fx:id="saveButton" layoutX="25.0" layoutY="201.0" mnemonicParsing="false" onAction="#saveGame" prefHeight="36.0" prefWidth="150.0" stylesheets="@../styles/Styles.css" text="Save">
         <effect>
            <DropShadow />
         </effect></Button>
      <Button fx:id="loadButton" layoutX="25.0" layoutY="261.0" mnemonicParsing="false" onAction="#loadGame" prefHeight="36.0" prefWidth="150.0" stylesheets="@../styles/Styles.css" text="Load">
         <effect>
            <DropShadow />
         </effect></Button>
      <TextField fx:id="cubeSizeTextField" editable="false" layoutX="25.0" layoutY="141.0" prefHeight="36.0" prefWidth="65.0" stylesheets="@../styles/Styles.css" text="Size:" />
   </children>
</AnchorPane>
